<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.karawalaya.alliantbankapp">

    <application
        android:allowBackup="true"
        android:icon="@drawable/bank_logo"
        android:label="@string/string_id_Action_Bar_Title"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ACTIVITIES_FRAGMENTS.MainActivity"></activity>
        <activity android:name=".ACTIVITIES_FRAGMENTS.Splash">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>